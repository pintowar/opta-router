{
  "name": "opta-router-webcli",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "generate-ts-api": "openapi-typescript http://localhost:8080/v3/api-docs.yaml --output src/api/generated/api.d.ts",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ./src/*.{js,vue,html,ts} ./src/**/*.{js,vue,html,ts} ./*.{js,html,ts}",
    "lint": "eslint --ext .ts,js,vue --ignore-pattern 'dist/**' ."
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "@vueuse/core": "^13.5.0",
    "apexcharts": "^4.7.0",
    "daisyui": "^5.0.46",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "oh-vue-icons": "^1.0.0-rc3",
    "rainbow-color": "^2.0.0",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue3-apexcharts": "^1.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/leaflet": "^1.9.20",
    "@types/lodash": "^4.17.20",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.3.0",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}
